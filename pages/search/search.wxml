<view class="content">
  <view class='nav bg-white' style='height:{{navH}}px'>
    <view class='nav-title'>
      搜索
      <!-- <image src='../../images/back.png' mode='aspectFit' class='back' bindtap='navBack'></image> -->
    </view>
  </view>
  <view class="search">
    <view class="rs">{{content.recent}}
      <view class='span'>{{content.search}}</view>
    </view>
    <view class="rs_li">
      <view class="li">
        <view class='a'>
          Olive Oil
        </view>
      </view>
      <view class="li">
        <view class='a'>
          Apple
        </view>
      </view>
      <view class="li">
        <view class='a'>
          Mustard Oil
        </view>
      </view>
      <view class="li">
        <view class='a'>
          Mango
        </view>
      </view>
    </view>
    <view class="rs">{{content.similar}}
      <view class='span'>{{content.products}}</view>
    </view>
    <view class="goods_list">
      <view class="li">
        <view class='a'>
          <view class="img_g">
            <image src='https://s3-us-west-1.amazonaws.com/scmteambucket/media/wechat/img_p_001.jpg' mode='widthFix'></image>
          </view>
          <view class="txt_g">
            <view class='span'>Grape Kyoho</view>
            <view class="pri">$9.99 </view>
            <view class="sma">{{content.box}}</view>
            <view class="btn_ad">{{content.add}}</view>
          </view>
          <view class="tip_g">
            12 Left in Stock
          </view>
          <view class="clr"></view>
        </view>
      </view>
      <view class="li">
        <view class='a'>
          <view class="img_g">
            <image src='https://s3-us-west-1.amazonaws.com/scmteambucket/media/wechat/img_p_001.jpg' mode='widthFix'></image>
          </view>
          <view class="txt_g">
            <view class='span'>Grape Kyoho</view>
            <view class="pri">$9.99 </view>
            <view class="sma">{{content.box}}</view>
            <view class="btn_ad">{{content.add}}</view>
          </view>
          <view class="tip_g">
            12 Left in Stock
          </view>
          <view class="clr"></view>
        </view>
      </view>
    </view>
  </view>
</view>