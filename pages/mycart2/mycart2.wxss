page {
  background: #f3f3f3;
}

.btn_round {
  margin: 15px 12px 5px;
}

.btn_round .ch_d {
  padding: 12px 0 0 40px;
  height: 40px;
  margin: 0 20px 0 0;
  background: url(http://bluablua.com/icon_white_ch.png) no-repeat;
  display: block;
  float: left;
  position: relative;
  color: #5c5c5c;
}

.btn_round .ch_d:hover, ul.sc_li .li .ch_d.hover {
  background: url(http://bluablua.com/icon_green_ch.png) no-repeat;
  color: #3aa900;
}

.box_gt {
  width: calc(100% - 24px);
  margin: 12px 12px 0;
  background: #fff;
  border: 1px solid #e0e0e0;
  border-radius: 5px;
  padding: 10px 12px;
}

.box_gt .he_t {
  margin: 0 0 10px 0;
}

.box_gt .he_t .l {
  text-align: left;
  display: block;
  float: left;
}

.box_gt .he_t .r {
  text-align: right;
  display: block;
  float: right;
}

.box_gt .b_l {
  display: flex;
}

.box_gt .b_l .li {
  flex: 1;
  margin: 0 5px 0 0;
}

.box_gt .b_l .li .a {
  font-size: 12px;
  color: #fff;
  text-align: center;
  display: block;
  padding: 6px 3px;
  border-radius: 5px;
  background: -webkit-linear-gradient(270deg, #aaa, #c8c8c8);
  background: -o-linear-gradient(270deg, #aaa, #c8c8c8);
  background: -moz-linear-gradient(270deg, #aaa, #c8c8c8);
  background: linear-gradient(270deg, #aaa, #c8c8c8);
}

.box_gt .b_l .li .a:hover, .box_gt .b_l .li .a.hover {
  background: -webkit-linear-gradient(270deg, #37a000, #43c102);
  background: -o-linear-gradient(270deg, #37a000, #43c102);
  background: -moz-linear-gradient(270deg, #37a000, #43c102);
  background: linear-gradient(270deg, #37a000, #43c102);
}

.box_gt .gt {
  margin: 10px 0;
  border-top: 1px solid #e0e0e0;
  padding: 10px 0;
}

.box_gt .gt .l {
  text-align: left;
  display: block;
  float: left;
}

.box_gt .gt .r {
  text-align: right;
  display: block;
  float: right;
  color: #f00;
}

.search {
  width: calc(100% - 24px);
  margin: 0 12px;
}

.search .go_li {
  border: 1px solid #e0e0e0;
  border-radius: 5px;
  margin: 16px 0;
}

.search .go_li .li {
  width: 100%;
  border-radius: 5px;
  border-bottom: 1px solid #e0e0e0;
}

.search .go_li .li {
  display: block;
  position: relative;
  background: #fff;
}

.search .go_li .li .img_g {
  width: 120px;
  margin: 12px;
  float: left;
}

.search .go_li .li .img_g image {
  width: 120px;
  display: block;
}

.search .go_li .li .txt_g {
  width: calc(100% - 172px);
  margin: 18px 12px 10px 0;
  float: left;
}

.search .go_li .li .txt_g .span {
  display: block;
  color: #333;
  margin: 6px 0;
}

.search .go_li .li .txt_g .pri {
  font-size: 16px;
  color: #ed0000;
}

.search .go_li .li .txt_g .sma {
  font-size: 12px;
  margin: 0 0 12px 0;
}

.search .go_li .li .rig {
  float: right;
  margin: 10px 0 0;
  position: absolute;
  right: 12px;
  top: 48px;
}

.search .go_li .li .rig .reduce {
  width: 21px;
  height: 21px;
  background: url(http://bluablua.com/icon_reduce.png) center center no-repeat;
  display: inline-block;
  background-size: 100%;
}

.search .go_li .li .rig .span {
  width: 24px;
  display: inline-block;
  text-align: center;
  line-height: 21px;
  position: relative;
  top: -4px;
  font-size: 18px;
}

.search .go_li .li .rig .add {
  width: 21px;
  height: 21px;
  background: url(http://bluablua.com/icon_add.png) center center no-repeat;
  display: inline-block;
  background-size: 100%;
}

.inp_ent {
  width: 100%;
  position: relative;
}

.inp_ent input {
  width: 100%;
  height: 36px;
  padding: 0 100px 0 0;
  line-height: 36px;
  background: transparent;
  border-bottom: 2px solid #e0e0e0;
}

.inp_ent .a {
  position: absolute;
  right: 5px;
  top: 5px;
  width: 80px;
  font-size: 12px;
  color: #fff;
  border-radius: 24px;
  height: 24px;
  line-height: 24px;
  text-align: center;
  display: block;
  background: -webkit-linear-gradient(270deg, #37a000, #43c102);
  background: -o-linear-gradient(270deg, #37a000, #43c102);
  background: -moz-linear-gradient(270deg, #37a000, #43c102);
  background: linear-gradient(270deg, #37a000, #43c102);
}

