page {
  background: url(http://bluablua.com/bg_reg.jpg) center center no-repeat;
  background-size: cover;
  background-attachment: fixed;
}

.register {
  width: 100%;
}

.register .content_log {
  width: calc(100% - 30px);
  margin: auto 15px;
  height: 432px;
  position: absolute;
  top: 0;
  bottom: 0;
}

.register .content_log .logo {
  width: 118px;
  height: 118px;
  margin: 0 auto 50px;
}

.register .content_log .logo image {
  width: 118px;
  margin: auto;
}

.register .content_log .inp_reg {
  padding: 20px 0;
  background: url(http://bluablua.com/line_reg.png) center bottom no-repeat;
  background-size: 100%;
  position: relative;
}

.register .content_log .inp_reg .lef {
  width: 24px;
  margin: 12px 24px 0 12px;
  float: left;
}

.register .content_log .inp_reg .lef image {
  width: 24px;
  display: block;
}

.register .content_log .inp_reg .rig {
  float: left;
  width: calc(100% - 60px);
}

.register .content_log .inp_reg .rig label {
  font-size: 14px;
}

.register .content_log .inp_reg .rig input {
  display: block;
  background: transparent;
  margin: 12px 0 0;
  width: 100%;
}

.register .content_log .inp_reg navigator.fg {
  display: block;
  position: absolute;
  right: 0;
  font-size: 12px;
  bottom: 10px;
  color: #1a1a1a;
  font-weight: 600;
}

.register .content_log .btn_log {
  width: 100%;
  margin: 30px auto 0;
}

.register .content_log .btn_log button {
  width: calc(100% - 20px);
  margin: 0 10px;
  color: #fff;
  display: block;
  height: 54px;
  text-align: center;
  line-height: 54px;
  border-radius: 54px;
  background: -webkit-linear-gradient(270deg, #37a000, #43c102);
  background: -o-linear-gradient(270deg, #37a000, #43c102);
  background: -moz-linear-gradient(270deg, #37a000, #43c102);
  background: linear-gradient(270deg, #37a000, #43c102);
}

/* Create Account */

