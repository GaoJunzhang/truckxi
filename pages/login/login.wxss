.register .content_ca view.am {
  width: 100%;
  text-align: center;
  margin: 70px 0 0;
}

.register .content_ca view.am view {
  width: 100%;
  text-align: center;
  color: #8b8b8b;
  font-weight: 100;
}

.register .content_ca view.log {
  margin: 8px 0 30px;
  display: block;
  font-weight: 600;
  width: 100%;
  text-align: center;
  color: #1a1a1a;
  font-size: 16px;
