.left_sider {
  width: 100%;
  position: fixed;
  bottom: 0;
  top: 0;
  left: 0;
  bottom: 0;
  margin: auto;
  background: rgba(0, 0, 0, 0.65);
}

.left_sider .cont_ls {
  width: calc(100% - 100px);
  margin: 0 100px 0 0;
  position: fixed;
  bottom: 0;
  top: 0;
  left: 0;
  bottom: 0;
  margin: auto;
  background: #fff;
}

.left_sider .cont_ls .image {
  width: 100%;
  position: relative;
}

.left_sider .cont_ls .image image {
  display: block;
  width: 100%;
}

.left_sider .cont_ls .image view.lang {
  font-size: 12px;
  height: 24px;
  display: block;
  padding: 0 20px 0 10px;
  border-radius: 5px;
  color: #4e4e4e;
  text-align: center;
  line-height: 24px;
  position: absolute;
  top: 10px;
  right: 12px;
  background: #fff url(https://s3-us-west-1.amazonaws.com/scmteambucket/media/wechat/icon_arrow_down.png) center right no-repeat;
  background-size: 18px;
}

.left_sider .cont_ls .image .head {
  position: absolute;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
  width: 120px;
  height: 48px;
  margin: auto;
  text-align: center;
}

.left_sider .cont_ls .image .head view {
  display: block;
}

.left_sider .cont_ls .image .head view image {
  width: 48px;
  height: 48px;
  display: block;
  margin: auto;
}

.left_sider .cont_ls .image .head text {
  width: 100%;
  color: #fff;
}

.left_sider .cont_ls .link_of {
  width: 100%;
  margin: 15px 0;
}

.left_sider .cont_ls .link_of .link_of_li,
.left_sider .cont_ls .link_tf .link_of_li {
  padding: 0 20px;
}

.left_sider .cont_ls .link_of .link_of_li navigator,
.left_sider .cont_ls .link_tf .link_of_li navigator {
  display: block;
  padding: 12px;
}

.left_sider .cont_ls .link_of .link_of_li navigator image,
.left_sider .cont_ls .link_tf .link_of_li navigator image {
  width: 16px;
  display: block;
  margin: 0 8px 0 0;
  float: left;
  position: relative;
  top: 2px;
}

.left_sider .cont_ls .link_of .link_of_li navigator text,
.left_sider .cont_ls .link_tf .link_of_li navigator text {
  font-size: 14px;
}

.left_sider .cont_ls .link_tf {
  width: 100%;
  margin: 15px 0;
  border-top: 1px solid #d8d8d8;
}

.left_sider .cont_ls .link_tf .oth {
  width: 100%;
  margin: 15px 0 10px;
  padding: 0 0 0 32px;
  color: #949494;
}

.Choose_yl {
  width: 100%;
  position: fixed;
  bottom: 0;
  top: 0;
  left: 0;
  margin: auto;
  background: rgba(0, 0, 0, 0.65);
}

.Choose_yl .box_c {
  width: calc(100% - 30px);
  height: 280px;
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  margin: auto;
  background: #fff;
  border-radius: 5px;
}

.Choose_yl .box_c .tit {
  width: 100%;
  font-size: 18px;
  margin: 25px 0;
  text-align: center;
}

.Choose_yl .box_c p.y {
  width: calc(100% - 100px);
  margin: 30px 50px 10px;
  text-align: center;
}

.Choose_yl .box_c input {
  width: calc(100% - 40px);
  margin: 20px 20px 10px 20px;
  border-bottom: 1px solid #aeaeae;
  text-align: center;
  padding: 10px 0;
}

.Choose_yl .box_c p.t {
  width: calc(100% - 100px);
  margin: 0 50px;
  font-size: 12px;
  text-align: center;
}

.Choose_yl .box_c view {
  width: 100%;
  display: block;
  margin: 25px 0 0;
  border-radius: 0 0 5px 5px;
  height: 42px;
  line-height: 42px;
  text-align: center;
  color: #fff;
  background: -webkit-linear-gradient(270deg, #37a000, #43c102);
  background: -o-linear-gradient(270deg, #37a000, #43c102);
  background: -moz-linear-gradient(270deg, #37a000, #43c102);
  background: linear-gradient(270deg, #37a000, #43c102);
}

.select_box {
  background: #fff;
  font-size: 12px;
  height: 24px;
  display: block;
  padding: 0 20px 0 10px;
  border-radius: 5px;
  color: #4e4e4e;
  text-align: center;
  line-height: 24px;
  position: absolute;
  top: 10px;
  right: 12px;
  background: #fff url(https://s3-us-west-1.amazonaws.com/scmteambucket/media/wechat/icon_arrow_down.png) center right no-repeat;
  background-size: 18px;
}

.select {
  box-sizing: border-box;
  width: 100%;
  /* height: 70rpx; *//* border:1px solid #efefef; */
  border-radius: 8rpx;
  display: flex;
  align-items: center;
  padding: 0 20rpx;
}

.select_text {
  font-size: 30rpx;
  flex: 1;
}

.select_img {
  width: 40rpx;
  height: 40rpx;
  display: block;
  transition: transform 0.3s;
}

.select_img_rotate {
  transform: rotate(180deg);
}

.option_box {
  position: absolute;
  /* top: 70rpx; *//* width: 100%; *//* border:1px solid #efefef; */
  box-sizing: border-box;
  height: 0;
  overflow-y: auto;
  border-top: 0;
  background: #fff;
  transition: height 0.3s;
}

.option {
  display: block;
  line-height: 40rpx;
  font-size: 30rpx;
  border-bottom: 1px solid #efefef;
  padding: 10rpx;
}
