.content {
  width: 100%;
}

.content .banner {
  width: 100%;
  position: relative;
}

.content .banner image {
  display: block;
  width: 100%;
}

.content .banner .round {
  position: absolute;
  bottom: 10px;
  text-align: center;
  margin: auto;
  left: 0;
  right: 0;
}

.content .banner .round view {
  width: 8px;
  height: 8px;
  color: #fff;
  border-radius: 100%;
  border: 1px solid #fff;
  display: inline-block;
  margin: 0 1px;
}

.content .banner .round view:hover, .content .round view.hover {
  background: #fff;
}

.content .category {
  /* width: calc(100% - 20px); */
  border: 1px solid #e0e0e0;
  border-radius: 5px;
  display: flex;
  width: 100%;
  box-sizing: border-box;
  overflow: hidden;
  background: white;
  font-size: 16px;
  white-space: nowrap;
  z-index: 99;
  border-bottom: 1px solid #e1e1e1;
}

.content .category .category_li {
  flex: 1;
}

.content .category .category_li view {
  display: block;
  margin: 12px 0;
  text-align: center;
}

.content .category .category_li view image {
  display: block;
  width: calc(100% - 30px);
  margin: 0 15px;
}

.content .category .category_li view text {
  text-align: center;
  padding: 5px 0 0;
  display: inline-block;
}

.content view {
  display: block;
}

.content view .ban_img {
  width: calc(100% - 20px);
  margin: 0 10px 15px;
  position: relative;
}

.content view .ban_img image {
  border-radius: 5px;
  display: block;
  width: 100%;
}

.content view .ban_img text.tl {
  position: absolute;
  left: 12px;
  bottom: 12px;
  color: #fff;
  font-size: 18px;
}

.content view .ban_img text.tr {
  position: absolute;
  right: 12px;
  bottom: 12px;
  color: #404040;
  display: block;
  padding: 5px 12px;
  background: #fff;
  border-radius: 40px;
  font-size: 12px;
}

.content .gd_s, .content .gd_h {
  width: 100%;
}

.content .gd_s .tit, .content .gd_h .tit {
  width: calc(100% - 20px);
  margin: 0 10px;
  font-size: 18px;
  font-weight: 400;
  position: relative;
}

.content .gd_s .tit text, .content .gd_h .tit text {
  color: #3aa700;
  font-size: 18px;
  font-weight: 400;
}

.content .gd_s .tit navigator.more, .content .gd_h .tit navigator.more {
  position: absolute;
  right: 0;
  bottom: 2px;
  width: 20px;
}

.content .gd_s .tit navigator.more image,
.content .gd_h .tit navigator.more image {
  display: block;
  width: 20px;
}

.content .gd_s .goods_list {
  width: calc(100% - 20px);
  margin: 0 10px 15px;
}

.content .gd_s .goods_list .goods_list_li {
  width: 100%;
  border-radius: 5px;
  box-shadow: 0 0 10px rgba(0, 0, 0, 0.2);
  margin: 15px 0;
}

.content .gd_s .goods_list .goods_list_li view {
  display: block;
  position: relative;
}

.content .gd_s .goods_list .goods_list_li view .img_g {
  width: 136px;
  margin: 12px;
  float: left;
}

.content .gd_s .goods_list .goods_list_li view .img_g image {
  width: 136px;
  display: block;
}

.content .gd_s .goods_list .goods_list_li view .txt_g {
  width: calc(100% - 172px);
  margin: 10px 12px 10px 0;
  float: left;
}

.content .gd_s .goods_list .goods_list_li view .txt_g text {
  display: block;
  color: #333;
  margin: 2px 0;
}

.content .gd_s .goods_list .goods_list_li view .txt_g text.pri {
  font-size: 16px;
  color: #ed0000;
}

.content .gd_s .goods_list .goods_list_li view .txt_g text.sma {
  font-size: 12px;
  margin: 0 0 12px 0;
}

