  left: 0;
  top: 0;
}

.content .gd_h .goods_infeed .goods_infeed_li navigator {
  display: block;
  position: relative;
}

.content .gd_h .goods_infeed .goods_infeed_li navigator .img_g {
  width: 100%;
  padding: 12px;
}

.content .gd_h .goods_infeed .goods_infeed_li navigator .img_g image {
  width: 100%;
  display: block;
}

.content .gd_h .goods_infeed .goods_infeed_li navigator .txt_g {
  width: 100%;
  padding: 0 12px;
  position: relative;
}

.content .gd_h .goods_infeed .goods_infeed_li navigator .txt_g text {
  display: block;
  color: #333;
